    "js-yaml": "^3.7.0",
    "node-uuid": "^1.4.7",
    "prettyjson": "^1.1.3",
    "superagent": "^2.3.0",
    "tar.gz": "^1.0.5",
    "thunkify": "^2.1.2",
    "traverse": "^0.6.6",
    "treeify": "^1.0.1",
    "yang-js": "^0.16.19"
  },
  "devDependencies": {
    "mocha": "~2.0.1",
    "should": "~3.1.3"
  },
  "engines": {
    "node": ">=0.10.28"
