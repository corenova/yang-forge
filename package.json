{
  "name": "yang-forge",
  "version": "2.0.0",
  "description": "YANG package manager and runtime engine",
  "keywords": [
    "yang",
    "restjson",
    "restconf",
    "build",
    "deploy",
    "translate",
    "package",
    "publish",
    "run",
    "engine",
    "core",
    "forge",
    "rfc6020",
    "yfc"
  ],
  "preferGlobal": true,
  "author": "Peter Lee <peter@corenova.com>",
  "homepage": "https://github.com/corenova/yang-forge",
  "license": "Apache-2.0",
  "repository": "corenova/yang-forge",
  "main": "lib/index.js",
  "bin": {
